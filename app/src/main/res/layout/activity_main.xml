<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lee.leedemo.MainActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp">

        <Button
            android:id="@+id/btn_bledemo"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="BLEDemo"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/btn_blephonedemo"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="BLEPhoneDemo"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/btn_kotlindemo"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="kotlinDemo"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="0dp" />

        <Button
            android:id="@+id/btn_annimationdemo"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="animation" />

        <Button
            android:id="@+id/btn_canvasdemo"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="cavans" />

        <Button
            android:id="@+id/btn_upvoteview"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="upvote" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
